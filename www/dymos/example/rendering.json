{
	"@context":"http://tiny.cc/dymo-context",
	"@id":"demoBeatRendering",
	"@@type":"Rendering",
	"dymo":"demoBeat",
	"mappings":[
		{
			"domainDims":[{
				"name":"Rhythmicality",
				"@type":"Slider",
				"value":0
			}],
			"dymos":["demoBeat"],
			"range":"Rhythmicality"
		},
		{
			"domainDims":[{
				"name":"geo",
				"@type":"GeolocationLatitude"
			}],
			"function":{"args":["a"],"body":"return a*10000;"},
			"dymos":["demoBeat"],
			"range":"Rhythmicality"
		},
		{
			"domainDims":[{
				"name":"Space",
				"@type":"Slider"
			}],
			"dymos":["demoBeat"],
			"range":"Space"
		},
		{
			"domainDims":[{
				"name":"Weirdness",
				"@type":"Slider"
			}],
			"dymos":["demoBeat"],
			"range":"Weirdness"
		}
	]
}