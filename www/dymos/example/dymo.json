{
	"@context":"http://tiny.cc/dymo-context",
	"@id":"demoBeat",
	"@type":"Dymo",
	"cdt":"Conjunction",
	"mappings":[
		{
			"domainDims":[{"@id":"Rhythmicality","@type":"CustomParameter"}],
			"function":{"args":["a"],"body":"return a/4;"},
			"targets":["bass", "drums", "flav"],
			"range":"Amplitude"
		},
		{
			"domainDims":["Rhythmicality"],
			"function":{"args":["a"],"body":"return 0.2-a;"},
			"targets":["pad"],
			"range":"Amplitude"
		},
		{
			"domainDims":[{"@id":"Space","@type":"CustomParameter"}],
			"function":{"args":["a"],"body":"return 2*a-1;"},
			"targets":["piano"],
			"range":"Pan"
		},
		{
			"domainDims":["Space"],
			"function":{"args":["a"],"body":"return -2*a+1;"},
			"targets":["pad"],
			"range":"Pan"
		},
		{
			"domainDims":["Space"],
			"function":{"args":["a"],"body":"return 5*a;"},
			"targets":["piano", "pad"],
			"range":"Reverb"
		},
		{
			"domainDims":["Space"],
			"function":{"args":["a"],"body":"return a;"},
			"targets":["drums", "bass"],
			"range":"Reverb"
		},
		{
			"domainDims":[{"@id":"Weirdness","@type":"CustomParameter"},{"@id":"brownian","@type":"Brownian"}],
			"function":{"args":["a","b"],"body":"return 1+a*(b*2-1);"},
			"targets":["blib","piano", "pad", "drums", "bass","flav"],
			"range":"PlaybackRate"
		},
		{
			"domainDims":["Weirdness","brownian"],
			"function":{"args":["a","b"],"body":"return a*(b-0.5);"},
			"targets":["piano", "flav"],
			"range":"Pan"
		},
		{
			"domainDims":["Weirdness","brownian"],
			"function":{"args":["a","b"],"body":"return a*(-b+0.5);"},
			"targets":["pad", "drums"],
			"range":"Pan"
		}
	],
	"parts":[
		{
			"@id":"pad",
			"@type":"Dymo",
			"source":"pad.m4a",
			"parameters":[
				{"@type":"Loop", "value":1},
				{"@type":"Amplitude", "value":0.2},
				{"@type":"PlaybackRate", "value":1}
			]
		},
		{
			"@id":"piano",
			"@type":"Dymo",
			"source":"piano.m4a",
			"parameters":[
				{"@type":"Loop", "value":1},
				{"@type":"Amplitude", "value":0.2},
				{"@type":"Reverb", "value":0.2},
				{"@type":"PlaybackRate", "value":1}
			]
		},
		{
			"@id":"bass",
			"@type":"Dymo",
			"source":"bass.m4a",
			"parameters":[
				{"@type":"Loop", "value":1},
				{"@type":"Amplitude", "value":0},
				{"@type":"Reverb", "value":0.2},
				{"@type":"PlaybackRate", "value":1}
			]
		},
		{
			"@id":"drums",
			"@type":"Dymo",
			"source":"drums.m4a",
			"parameters":[
				{"@type":"Loop", "value":1},
				{"@type":"Amplitude", "value":0},
				{"@type":"PlaybackRate", "value":1}
			]
		},
		{
			"@id":"flav",
			"@type":"Dymo",
			"source":"flav.m4a",
			"parameters":[
				{"@type":"Loop", "value":1},
				{"@type":"Amplitude", "value":0},
				{"@type":"PlaybackRate", "value":1}
			]
		},
		{
			"@id":"blib",
			"@type":"Dymo",
			"source":"blib.m4a",
			"parameters":[
				{"@type":"Loop", "value":1},
				{"@type":"Amplitude", "value":0.2},
				{"@type":"PlaybackRate", "value":0.7}
			]
		}
	]
}